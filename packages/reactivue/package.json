{
  "name": "@lyonbot/reactivue",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "description": "Using Vue Composition API in React components.",
  "author": "Anthony Fu<https://github.com/antfu>",
  "license": "MIT",
  "version": "0.4.5",
  "browser": "dist/index.module.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": {
        "production": "./dist/index.module.js",
        "development": "./dist/index.module.dev.js"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./": "./",
    "./preact": {
      "types": "./dist/index.d.ts",
      "browser": {
        "production": "./preact/index.module.js",
        "development": "./preact/index.module.dev.js"
      },
      "import": "./preact/index.mjs",
      "require": "./preact/index.js"
    }
  },
  "files": [
    "dist",
    "preact"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyonbot/reactivue.git"
  },
  "keywords": [
    "vue",
    "react",
    "vue-composition-api",
    "vue-in-react",
    "reactivity"
  ],
  "bugs": {
    "url": "https://github.com/lyonbot/reactivue/issues"
  },
  "homepage": "https://github.com/lyonbot/reactivue#readme",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^26.0.22",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel-jest": "^26.6.3",
    "cac": "^6.7.2",
    "jest": "^26.6.3",
    "preact": "^10.5.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.0",
    "rollup": "^2.44.0",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "preact": ">=10",
    "react": ">=16"
  },
  "dependencies": {
    "@vue/reactivity": "^3.4.27",
    "@vue/runtime-core": "^3.4.27",
    "@vue/shared": "^3.4.27"
  },
  "peerDependenciesMeta": {
    "preact": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  }
}
